{
	"expo": {
		"name": "JetBeans",
		"slug": "jetbeans",
		"version": "0.1.0",
		"scheme": "jetbeans",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#654321"
		},
		"ios": {
			"bundleIdentifier": "cafe.jetbeans.mobile",
			"supportsTablet": true
		},
		"android": {
			"package": "cafe.jetbeans.mobile",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#654321"
			}
		},
		"plugins": ["expo-router", "expo-secure-store"]
	}
}
